      <memberdef kind="function" id="Messenger_8h_1a51b1f8270e728dfe799b7e6558590996" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>bool</type>
        <definition>bool operator&lt;</definition>
        <argsstring>(const BMessenger &amp;a, const BMessenger &amp;b)</argsstring>
        <name>operator&lt;</name>
        <param>
          <type>const <ref refid="classBMessenger" kindref="compound">BMessenger</ref> &amp;</type>
          <declname>a</declname>
